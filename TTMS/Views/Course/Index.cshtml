@model IEnumerable<TMS.Models.Course>
@{
    ViewBag.Title = "Index";
}

<h2>Manage Course</h2>
<h3 class="btn btn-primary">@Html.ActionLink("Create Course", "Create", "Course")</h3>

<table class="table table-bordered">
    <thead>
        <tr>
            <td>#</td>
            <td>Name</td>
            <td>Category</td>
            <td>Description</td>
        </tr>

    </thead>

    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Categogy</td>
                <td>@item.Description</td>

            </tr>
        }
    </tbody>
</table>

